                  <%}else{%>
                    <div class="joint text-small visibility-none">Mirror</div>
                    <% }%>
                      <table class="mb-10">
                        <tr>
                          <td class="key">Name</td>
                          <td class="key">Gender</td>
                        </tr>
                        <tr>
                          <td>
                            <%= exclusion.name ?? '' %>
                          </td>
                          <td>
                            <%= exclusion.gender ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table class="mb-10">
                        <tr>
                          <td class="key">DOB</td>
                          <td class="key">Relationship</td>
                        </tr>
                        <tr>
                          <td>
                            <%=moment(exclusion.dob ?? '' ).format('DD /MM/YYYY')%>
                          </td>
                          <td>
                            <%=exclusion.relationship ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table class="mb-10">
                        <tr>
                          <td class="key" colspan="2">Reason of exclusion</td>
                        </tr>
                        <tr>
                          <td colspan="2">
                            <%=exclusion.reasonOfExclusion ?? '' %>
                          </td>
                        </tr>
                      </table>
              </div>
              <%});%>
          </div>
          <%});%>
      </div>
      <hr>
    </div>

    <!-- BUSINESS INTRESTS -->
    <div class="section">
      <div class="section_heading">Business Interests</div>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <table class="mb-10">
              <tr>
                <td class="pb-15">Are you including power to carry on business?</td>
                <td class="pb-15 text-green">
                  <%= willQuestionnaireIns?.businessInterests?.carryBusinessPower===true?'Yes':'No' %>
                </td>
              </tr>
            </table>
          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- FUNERAL WISHES -->
    <div class="section">
      <div class="section_heading">Funeral Wishes</div>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <table class="">
              <tr>
                <td class="key">Funeral Type</td>
              </tr>
              <tr>
                <td class="">
                  <%= willQuestionnaireIns.funeralWishes.type ?? '' %>
                </td>
              </tr>
            </table>
            <table class="">
              <tr>
                <td class="key">Do you wish to specify any funeral requests?</td>
              </tr>
              <tr>
                <td class="">
                  <%= willQuestionnaireIns.funeralWishes.requests ?? '' %>
                </td>
              </tr>
            </table>
          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- Additional or Useful Information & Explanatory Notes -->
    <div class="section">
      <div class="section_heading">Additional or Useful Information & Explanatory Notes</div>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <div class="text-wrap">
              <%= willQuestionnaireIns.informationNotes.notes ?? '' %>
            </div>

          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

</body>

</html>