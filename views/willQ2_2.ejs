  }

  .text-small {
    font-size: 12px;
  }

  .text-bold {
    font-weight: 600;
  }

  .text-normal {
    font-weight: normal;
  }

  .text-dark {
    color: #3D4740 !important;
  }



  .my-10 {
    margin: 10px 0px;
  }

  .my-20 {
    margin: 20px 0px;
  }

  .mx-20 {
    margin: 0px 20px;
  }

  .py-20 {
    padding: 20px 0px;
  }

  .m-20 {
    margin: 20px;
  }

  .mb-5 {
    margin-bottom: 5px;
  }

  .mt--5 {
    margin-top: -5px;
  }

  .mt-10 {
    margin-top: 10px;
  }

  .mb-10 {
    margin-bottom: 10px;
  }

  .mb-20 {
    margin-bottom: 20px;
  }

  .mb-30 {
    margin-bottom: 30px;
  }

  .mr-30 {
    margin-right: 30px;
  }

  .mx-15p {
    margin: 0px 15%;
  }

  .px-5 {
    padding: 0px 5px;
  }

  .px-20 {
    padding: 0px 20px;
  }

  .pb-8 {
    padding-bottom: 8px;
  }

  .pb-13 {
    padding-bottom: 13px;
  }

  .pb-15 {
    padding-bottom: 8px;
  }

  .px-15 {
    padding: 0px 15px;
  }

  .p-10 {
    padding: 10px;
  }

  .p-20 {
    padding: 20px;
  }

  .pr-10 {
    padding-right: 10px;
  }

  .pr-20 {
    padding-right: 20px;
  }

  .flex-1 {
    flex: 1;
  }

  .flex-3 {
    flex: 3;
  }

  .text-right {
    text-align: right;
  }

  .text-left {
    text-align: left;
  }

  .text-center {
    text-align: center;
  }

  .text-top {
    vertical-align: top;
  }

  .vertical-center {
    vertical-align: middle !important;
  }

  .text-underline {
    padding-bottom: 5px;
    margin: 5px;
    border-bottom: 1px solid lightgray;
  }

  .text-green {
    color: #00CE3F;
  }

  .text-white {
    color: white;
  }

  .table_container {
    display: flex;
    break-inside: avoid;
  }

  .exclusion_table_container {
    padding: 10px;
    box-sizing: border-box;
    border: 1px solid lightgray;
    border-radius: 4px;
    break-inside: avoid;
    display: flex;
    flex-direction: column;
    position: relative;
    width: 100%;
  }

  hr {
    margin: 0px 20px;
    border-top: 0.5px solid lightgray;
  }

  .hr {
    border-top: 1px solid #DCDCE1;
    margin: 0px 20px;
  }

  .width-20 {
    width: 20%;
  }

  .width-33 {
    width: 33% !important;
  }


  .width-50 {
    width: 50%;
  }

  .width-60 {
    width: 60%;
  }

  .width-25 {
    width: 25%;
  }

  .width-70 {
    width: 70%;
  }

  .width-70 {
    width: 70%;
  }

  .width-85 {
    width: 85%;
  }

  .vert-align-top {
    vertical-align: text-top;
  }

  table {
    width: 100%;
  }

  .custom_table {
    width: 100%;
    padding: 20px;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    gap: 20px;
    border-right: 1px solid lightgray;
    break-inside: avoid;
  }

  .custom_table:nth-child(even) {
    border-right: none;
  }

  .custom_table_half {
    width: 50%;
    padding: 20px;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    gap: 20px;
    break-inside: avoid;
    border-right: 1px solid lightgray;
  }

  .custom_table_half:nth-child(even) {
    border-right: none;
  }

  .visibility-none {
    visibility: hidden;
  }

  .joint {
    border: 1px solid #00CE3F;
    width: max-content;
    background-color: #00CE3F;
    color: white;
    padding: 1px 4px;
    border-radius: 14px;
    position: absolute;
    right: 7px;
    top: 7px;
    font-size: 12px;
  }

  .declaration_container {
    display: flex;
    gap: 20px;
    width: 85%;
    margin-bottom: 30px;
    vertical-align: text-top;
  }

  .checkbox {
    border-radius: 4px;
    background-color: #00CE3F;
    color: white;
    height: 25px;
    width: 50px;
    font-size: 18px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 10px;
  }

  .signature_box {
    height: 120px;
    width: 350px;
    border: 1px solid lightgray;
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .signature_box_appointment_declaration {
    height: 120px;
    width: 100%;
    border: 1px solid lightgray;
    border-radius: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .address_document_image {
    height: 120px;
    width: 350px;
    border: 1px solid lightgray;
    border-radius: 15px;
    display: flex;
  }

  .center_container {
    display: flex;
    justify-content: center;
    padding: 20px;
  }

  .min-width-80 {
    min-width: 100%;
  }

  .round_div_with_padding_20 {
    padding: 10px;
    box-sizing: border-box;
    border: 1px solid lightgray;
    border-radius: 4px;
    break-inside: avoid;
    display: flex;
    flex-direction: column;
    position: relative;
  }

  .info_container {
    background-color: #E5FAEB;
    display: flex;
    padding: 15px;
    border-radius: 10px;
    gap: 10px;
  }

  .info {
    border-radius: 50%;
    padding: 5px;
    border: 1px solid rgb(65, 64, 64);
    height: 10px;
    width: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 5px;
  }

  .address_card {
    width: 45%;
    padding: 15px;
    border-radius: 15px;
    background-color: #00CE3F;
  }

  .address_card_relatives {
    width: 45%;
    padding: 15px;
    border-radius: 15px;
    border: 1px solid lightgray;
  }

  .text-wrap{
    overflow-wrap: break-word;
  }
</style>

<body>
  <!-- header  -->
  <header class="header">
    <div class="header_top">
      <div class="form_name_div">
        <span class="form_name">Will Questionnaire</span>
      </div>
    </div>
    <div class="header_bottom">
      <div class="header_bottom_left">
        <p class="key">Client 1</p>
        <p class="value text-style text-bold">
          <%= data?.parties[0]?.firstName +' '+ data?.parties[0]?.lastName %>
        </p>
      </div>
      <% if (data?.applicationType==="Joint" ) { %>
      <div class="header_bottom_right">
        <p class="key">Client 2</p>
        <p class="value text-style text-bold">
          <%= data?.parties[1]?.firstName +' '+ data.parties[1]?.lastName %></p>
      </div>
      <% } %>
    </div>
  </header>

  <!-- TESTATORS section -->
  <div class="section">
    <p class="section_heading">Testators</p>
    <div class="hr"></div>
    <div class="table_container">
      <% data?.willQuestionnaire?.forEach(function(willQuestionnaireIns, index){%>
      <div class=<%= data?.applicationType === ' Joint' ? "custom_table_half" : "custom_table" %>>
            <table>
              <tr>
                <td class="key">Testator <%= index + 1 %>
                </td>
              </tr>
              <tr>
                <td>
                  <%= willQuestionnaireIns.testators.fullName ?? '' %>
                </td>
              </tr>
            </table>
            <table>
              <tr>
                <td class="key">Do you wish your Spouse/partner to be an Executor/Trustee?
                </td>
              </tr>
              <tr>
                <td>
                  <%= willQuestionnaireIns.testators.makeExecutorOrTrustee ?? '' %>
                </td>
              </tr>
            </table>
            <table>
              <tr>
                <td class="key">Marriage Clause</td>
              </tr>
              <tr>
                <td>
                  <%= willQuestionnaireIns.testators.marriageClause===true ? 'Yes' : 'No' %>
                </td>
              </tr>
            </table>
      </div>
      <%});%>
    </div>
    <div class="hr"></div>
    </div>

    <!-- EXECUTORS & TRUSTEES -->
    <div class="section">
      <p class="section_heading">Executors & Trustees</p>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire?.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <% willQuestionnaireIns?.executorsAndTrustees?.forEach(function(executors, ind=index){%>
              <div class="exclusion_table_container">
                <% if(index===0 && executors?.isJoint ){%>
                  <div class="joint text-small">Joint</div>
                  <%}else{%>
                    <div class="joint text-small visibility-none">Joint</div>
                    <% }%>
                      <table class="">
                        <tr>
                          <td class="key">Full Name</td>
                        </tr>
                        <tr>
                          <td>
                            <%= executors.fullName ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table>
                        <tr>
                          <td class="key">Role</td>
                        </tr>
                        <tr>
                          <td>
                            <%= executors.role ?? '' %>
                          </td>
                        </tr>
                      </table>
              </div>
              <%});%>

          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- PROFESSIONAL EXECUTOR -->
    <div class="section">
      <p class="section_heading">Professional Executor</p>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire?.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
              <table class="">
                <tr>
                  <td class="key">Selected Executor</td>
                </tr>
                <tr>
                  <td>
                    <%= data?.willQuestionnaire[0]?.professionalExecutor?.executors %>
                  </td>
                </tr>
              </table>
              <% if(data.willQuestionnaire[0].professionalExecutor?.executors==='Other' ){%>
                <table>
                  <tr>
                    <td class="key">Full Name</td>
                    <td class="key" colspan="2">Address</td>
                  </tr>
                  <tr>
                    <td>
                      <%= data.willQuestionnaire[0]?.professionalExecutor?.fullName %>
                    </td>
                    <td colspan="2">
                      <%= data.willQuestionnaire[0]?.professionalExecutor?.address.street1 +
                        data.willQuestionnaire[0]?.professionalExecutor?.address.street2 +','+
                        data.willQuestionnaire[0]?.professionalExecutor?.address.city +','+
                        data.willQuestionnaire[0]?.professionalExecutor?.address.state +','+
                        data.willQuestionnaire[0]?.professionalExecutor?.address.zip +','+
                        data.willQuestionnaire[0]?.professionalExecutor?.address.country %>
                    </td>
                  </tr>
                </table>
                <%}%>

                    <table>
                      <tr>
                        <td class="key" colspan="2">Are you aware of fees?</td>
                      </tr>
                      <tr>
                        <td colspan="2">
                          <%= data.willQuestionnaire[0]?.professionalExecutor?.feesAwareness===true ? 'Yes' :'No' %>
                        </td>
                      </tr>
                    </table>
                    
          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- GUARDIANS (on second death) -->
    <div class="section">
      <div class="section_heading">
        <div class="text-big text-green text-bold">Guardians
          <span class="text-small text-normal">
            (on second death)
          </span>
        </div>
      </div>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire?.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <% willQuestionnaireIns?.guardians?.forEach((guar, ind=index)=> { %>
              <div class="exclusion_table_container">
                <% if(index===0 && guar?.isJoint){%>
                  <div class="joint text-small">Joint</div>
                  <%}else{%>
                    <div class="joint text-small visibility-none">Joint</div>
                    <% }%>

                      <table class="">
                        <tr>
                          <td class="key">Guardian <%= ind+1 %>
                          </td>
                        </tr>
                        <tr>
                          <td>
                            <%= guar.fullName ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <% if(ind !==0) {%>
                        <table class="">
                          <tr>
                            <td class="key">Going to act:</td>
                          </tr>
                          <tr>
                            <td>
                              <%= guar.goingToAct ?? '' %>
                            </td>
                          </tr>
                        </table>
                        <%}else{%>
                          <% }%>
                            <table>
                              <tr>
                                <td class="key">Guardian For</td>
                              </tr>
                              <tr>


                                <td>
                                  <% guar?.guardianFor?.forEach((child, indi)=> { %>
                                    <% if(indi !==0) {%>
                                      , <%=child%>
                                        <%}else{%>
                                          <%=child%>
                                            <%}%>
                                              <% }) %>
                                </td>
                              </tr>
                            </table>
              </div>
              <% }) %>
          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- Chattels and Gifts / Specific Legacies -->
    <div class="section">
      <p class="section_heading">Chattels and Gifts / Specific Legacies</p>
      <div class="section_sub">
        <div class="section_sub_heading mb-5">Expression of Wishes</div>
        <div class="section_sub_text">You may leave your chattels and personal items “to be distributed
          according to any expression of wishes”. Your Trustees will then carry out your wishes as directed.
        </div>
      </div>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire?.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <% willQuestionnaireIns?.chattlesAndGifts?.forEach((chattlesgifts, ind=index)=> { %>
              <div class="exclusion_table_container">
                <% if(index===0 && chattlesgifts?.isJoint){%>
                  <div class="joint text-small">Joint</div>
                  <%}else{%>
                    <div class="joint text-small visibility-none">Joint</div>
                    <% }%>

                      <table class="">
                        <tr>
                          <td class="key">Recipient Name</td>
                        </tr>
                        <tr>
                          <td>
                            <%= chattlesgifts.recipientName ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table class="">
                        <tr>
                          <td class="key">Relationship</td>
                        </tr>
                        <tr>
                          <td>
                            <%= chattlesgifts.relationship ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table>
                        <tr>
                          <td class="key">Gift Details</td>
                        </tr>
                        <tr>
                          <td>
                            <%= chattlesgifts.giftDetails ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table>
                        <tr>
                          <td class="key">Per Stirpes</td>
                          <td class="key">On death of</td>
                        </tr>
                        <tr>
                          <td>
                            <%= chattlesgifts.perStripes===true?'Yes':'No' %>
                          </td>
                          <td>
                            <%= chattlesgifts.onDeathOf ?? '' %>
                          </td>
                        </tr>
                      </table>
              </div>
              <% }) %>

          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- MONETARY LEGACIES -->
    <div class="section">
      <p class="section_heading">Monetary Legacies</p>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire?.forEach((willQuestionnaireIns, index)=> { %>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <% willQuestionnaireIns?.monetaryLegacies?.forEach((monetarylegacy, ind=index)=> { %>
              <div class="exclusion_table_container">
                <% if (index===0 && monetarylegacy?.isJoint) { %>
                  <div class="joint text-small">Joint</div>
                  <% } else { %>
                    <div class="joint text-small visibility-none">Joint</div>
                    <% } %>
                      <table class="">
                        <tr>
                          <td class="key">Recipient Name</td>
                        </tr>
                        <tr>
                          <td>
                            <%= monetarylegacy?.recipientName %>
                          </td>
                        </tr>
                      </table>
                      <table class="">
                        <tr>
                          <td class="key">Relationship</td>
                        </tr>
                        <tr>
                          <td>
                            <%= monetarylegacy.relationship ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table>
                        <tr>
                          <td class="key">Gift Details</td>
                        </tr>
                        <tr>
                          <td>
                            <%= monetarylegacy.giftDetails ?? '' %>
                          </td>
                        </tr>
                      </table>
                      <table>
                        <tr>
                          <td class="key">Per Stripes</td>
                          <td class="key">On Death of</td>
                        </tr>
                        <tr>
                          <td>
                            <%= monetarylegacy.perStripes===true?'Yes':'No' %>
                          </td>
                          <td>
                            <%= monetarylegacy.onDeathOf ?? '' %>
                          </td>
                        </tr>
                      </table>
              </div>
              <% }) %>
          </div>
          <% }) %>
      </div>
      <div class="hr"></div>
    </div>

    <!-- WILL TRUSTS -->
    <div class="section">
      <p class="section_heading">Will Trusts</p>
      <div class="hr"></div>
      <div class="table_container">
        <% data?.willQuestionnaire.forEach(function(willQuestionnaireIns, index){%>
          <div class=<%=data?.applicationType==='Joint' ? "custom_table_half" : "custom_table" %>>
            <table class="">
              <tr>
                <td class="key">Reason for establishing the Will Trust</td>
              </tr>
              <tr>
                <td>
                  <%= willQuestionnaireIns?.willTrusts?.willTrustEstablishmentReason %>
                </td>
              </tr>
            </table>
            <table class="">
              <tr>
                <td class="key">Does the client wish to include a clause in their Will to preserve their
                  Residence Nil Rate Band ?</td>
              </tr>
              <tr>
                <td>
                  <%= willQuestionnaireIns.willTrusts?.isClauseIncluded===true?'Yes':'No' %>
                </td>
              </tr>
            </table>
          </div>
          <%});%>
      </div>
      <div class="hr"></div>
    </div>

    <!-- FLEXIBLE LIFE INTRESTS -->
    <div class="section">
      <div class="flex-row-sb">
        <div class="section_sub">
          <div class="section_heading">
            <div class="text-big text-bold">Flexible Life Interest Trust “FLIT”
              <span class="text-small text-normal">
                (Recommended if the client have their own "What if...?")
              </span>
            </div>
            <div class="key mt-10">Giving a person a right to reside for a set period of time after death of
              both parents.
            </div>
          </div>
        </div>
        <div class="text-green mr-30">
          <%= data?.willQuestionnaire[0]?.willTrusts.isFLIT===true?'Yes':'No' %>
        </div>
      </div>
      <% if(data?.willQuestionnaire[0]?.willTrusts.isFLIT){%>
        <div class="hr"></div>
        <div class="table_container">
